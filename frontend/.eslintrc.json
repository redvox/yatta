{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "error",
    "quotes": [
      "error",
      "single"
    ],
    "no-unused-vars": "error",
    "consistent-return": "error",
    "curly": "error",
    "default-case": "error",
    "eqeqeq": "error",
    "no-global-assign": "error",
    "no-invalid-this": "error",
    "no-return-assign": "error",
    "no-unused-expressions": "error",
    "no-catch-shadow": "error",
    "no-undefined": "error",
    "no-use-before-define": "error",
    "no-duplicate-imports": "error",
    "no-var": "error",
    "prefer-arrow-callback": "warn",
    "prefer-const": "warn",
    "no-useless-rename": "warn",
    "no-confusing-arrow": "warn",
    "arrow-spacing": "warn",
    "no-useless-escape": "warn",
    "no-else-return": "warn",
    "no-alert": "warn",
    "dot-notation": "warn",
    "dot-location": [
      "warn",
      "property"
    ]
  },
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ]
}
